        <uuid>_a-0000dbbe-5b0d-8000-5130-010000010000_4930</uuid>
        <description>For an identifier, finds its index in the identifiers array and then returns the corresponding label.</description>
        <parentUuid>_a-0000dbbe-5b0d-8000-5130-010000010000_4918</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=index(ri!labels, wherecontains(ri!identifier, ri!identifiers))</definition>
