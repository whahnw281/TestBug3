<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>BW_ucArrayPickerFilter</name>
        <uuid>_a-0000dbbe-5b0d-8000-5130-010000010000_4928</uuid>
        <description>cans labels that match the text entered by the user and returns a DataSubset for use in the SAIL picker component.</description>
        <parentUuid>_a-0000dbbe-5b0d-8000-5130-010000010000_4918</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=with(&#xD;
  local!matches: where(apply(search(ri!filter, _), ri!labels)),&#xD;
  'type!{http://www.appian.com/ae/types/2009}DataSubset'(data: index(ri!labels, local!matches), identifiers: index(ri!identifiers, local!matches))&#xD;
)</definition>
        <namedTypedValue>
            <name>filter</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>labels</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>identifiers</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
